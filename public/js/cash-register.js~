(function() {
  var Dish;

  window.Dish = Dish = (function() {

    function Dish(rawDescription) {
      if (rawDescription == null) {
        rawDescription = "";
      }
      this.title = rawDescription.match(/([^$]+)/)?[1].trim();
    }

    return Dish;

  })();

}).call(this);
